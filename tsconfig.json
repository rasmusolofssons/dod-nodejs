{
    "compilerOptions": {
        "module": "commonjs",
        "noImplicitAny": false,
        "preserveConstEnums": true,
        "experimentalDecorators": true,
        "sourceMap": true,
        "allowJs": true,
        "moduleResolution": "node",
        "target": "es2017",
        "incremental": true,
        "types": [
            "node",
        ],
        "outDir": "dist",
    },
    "include": [
        "src/*/",
    ],
}